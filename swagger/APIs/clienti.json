  "paths": {
    "/clienti/oauth/token": {
      "post": {
        "summary": "Request Access Token",
        "description": "Token request must be authenticated using Basic authentication, with the client ID as the username and client secret as the password. That is, the token request must include the following header:\n\n    Authorization: Basic base64(client_id:client_secret)\n\n**When testing the API call in Swagger UI, click the lock icon to specify the client ID and secret for authentication.**",
        "security": [
          {
            "TokenRequestBasicAuth": []
          }
        ],
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "parameters": [
          {
            "name": "grant_type",
            "x-go-name": "grant_type",
            "format": "base64",
            "in": "formData",
            "description": "Value MUST be set to \"client_credentials\"",
            "required": true,
            "type": "string",
            "enum": [
              "client_credentials"
            ]
          },
          {
            "name": "client_id",
            "format": "base64",
            "x-go-name": "client_id",
            "in": "formData",
            "description": "The code received in the query string when redirected from authorization page",
            "required": true,
            "type": "string"
          },
          {
            "name": "client_secret",
            "x-go-name": "ClientSecret",
            "format": "base64",
            "in": "formData",
            "description": "The code received in the query string when redirected from authorization page",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "403": {
            "description": "Forbidden",
            "schema": {
              "type": "object",
              "properties": {
                "access_token": {
                  "$ref": "#/definitions/VARCHAR2"
                }
              }
            }
          }
        }
      }
    },
